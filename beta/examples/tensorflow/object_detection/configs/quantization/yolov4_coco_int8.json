{
    "model": "Yolov4",
    "input_info": {
        "sample_size": [1, 320, 320, 3]
    },

    "epochs": 1,
    "batch_size": 4,
    "weight_decay": 0.0001,

    "dataset": "coco/2017",
    "dataset_type": "tfrecords",

    "optimizer": {
        "type": "sgd",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [
                1
            ],
            "values": [
                1e-4,
                1e-4
            ]
        }
    },

    "compression": {
        "algorithm": "quantization"
    }
}

